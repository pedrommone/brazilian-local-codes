{
    "name": "pedrommone/brazilian-local-codes",
    "type": "library",
    "description": "A full indexed local codes by city and state",
    "version": "0.1.0",
    "keywords": [
        "local-codes",
        "brazilian"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.0|^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "~7.0",
        "phpunit/phpunit": "^9.2",
        "symfony/console": "~5.1",
        "symfony/css-selector": "~5.1",
        "symfony/dom-crawler": "~5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Pedrommone\\BrazilianLocalCodes\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
